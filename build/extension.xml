<extension xmlns="http://ns.adobe.com/air/extension/20.0"> 
    <id>com.digitalstrawberry.nativeExtensions.ANEAmplitude</id> 
    <versionNumber>1.2.2</versionNumber> 
    <platforms>
        
	<platform name="iPhone-ARM"> 
            <applicationDeployment> 
                <nativeLibrary>libANEAmplitude.a</nativeLibrary> 
                <initializer>ANEAmplitudeInitializer</initializer>
		<finalizer>ANEAmplitudeFinalizer</finalizer>
            </applicationDeployment> 
        </platform>
	
	<platform name="iPhone-x86"> 
            <applicationDeployment> 
                <nativeLibrary>libANEAmplitude.a</nativeLibrary> 
                <initializer>ANEAmplitudeInitializer</initializer>
		<finalizer>ANEAmplitudeFinalizer</finalizer>
            </applicationDeployment> 
        </platform>
	
	<platform name="Android-ARM">
	    <applicationDeployment>
		<nativeLibrary>libANEAmplitude.jar</nativeLibrary>
		<initializer>com.digitalstrawberry.nativeExtensions.aneamplitude.ANEAmplitude</initializer>
		<finalizer>com.digitalstrawberry.nativeExtensions.aneamplitude.ANEAmplitude</finalizer>
	    </applicationDeployment>
	</platform>
	
	<platform name="Android-x86">
	    <applicationDeployment>
		<nativeLibrary>libANEAmplitude.jar</nativeLibrary>
		<initializer>com.digitalstrawberry.nativeExtensions.aneamplitude.ANEAmplitude</initializer>
		<finalizer>com.digitalstrawberry.nativeExtensions.aneamplitude.ANEAmplitude</finalizer>
	    </applicationDeployment>
	</platform>
	
	<platform name="default"> 
	    <applicationDeployment/> 
	</platform> 
    
    </platforms> 
</extension>